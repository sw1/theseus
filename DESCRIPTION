Package: theseus
Type: Package
Title: Analysis and Visualization Tools for Microbial Community Data
Version: 0.0.900
Authors@R: c(
    person("Jacob", "Price", email = "jrp355@drexel.edu", role = c("cre","aut")),
    person("Stephen", "Woloszynek", email = "sw424@drexel.edu", role = c("aut"))
